{
    "name": "hello-wasm",
    "version": "0.1.0",
    "dependencies": {
        "assemblyscript": "github:nearprotocol/assemblyscript",
        "near-runtime-ts": "github:nearprotocol/near-runtime-ts"
    },
    "scripts": {
        "build": "npm run build:as && cp ./out/main.wasm ../hello.wasm",
        "build:as": "asc assembly/main.ts -o out/main.wasm -t out/main.wat --runtime stub -O3 --validate --measure"
    }
}
